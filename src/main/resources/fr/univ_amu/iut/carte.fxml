<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<GridPane id="gridpane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univ_amu.iut.Jdbc">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="20.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="80.0" />
    </columnConstraints>
    <VBox fx:id="vbox" alignment="CENTER" spacing="15" style="-fx-background-color: rgb(182,182,255);" GridPane.columnIndex="0">
        <padding>
            <Insets topRightBottomLeft="15" />
        </padding>
    </VBox>
    <HBox GridPane.columnIndex="1" alignment="CENTER">
        <VBox alignment="CENTER">
            <StackPane fx:id="carte">
            </StackPane>
        </VBox>
    </HBox>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
