<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane id="gridpane" style="-fx-background-color: white" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univ_amu.iut.Jdbc">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="20.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="80.0" />
    </columnConstraints>
    <VBox fx:id="vbox" alignment="CENTER" prefWidth="33.0" spacing="15" style="-fx-background-color: rgb(182,182,255);" GridPane.rowSpan="2">
        <padding>
            <Insets topRightBottomLeft="15" />
        </padding>
    </VBox>
    <HBox alignment="CENTER" GridPane.columnIndex="1">
        <Label alignment="CENTER" prefHeight="31.0" prefWidth="436.0" text="Cartographie des usages" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
        <Button fx:id="fermer" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#switchTo" prefHeight="31.0" prefWidth="31.0" style="-fx-background-color: transparent;" styleClass="closeButton" stylesheets="@style.css" userData="accueil.fxml" HBox.hgrow="SOMETIMES">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
            <graphic>
                <ImageView fitHeight="18.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@fermer.png" />
                    </image>
                    <viewport>
                        <Rectangle2D minX="40.0" minY="40.0" />
                    </viewport>
                </ImageView>
            </graphic>
        </Button>
    </HBox>
    <VBox alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <HBox alignment="CENTER">
            <StackPane fx:id="carte">
            </StackPane>
        </HBox>
    </VBox>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints />
   </rowConstraints>
</GridPane>
